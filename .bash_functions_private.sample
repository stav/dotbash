# .bash_functions_private.sample:
# rename this file to ~/.bash_functions_private

SW_HEADER_PRIVATE='
  PRIVATE HANDLES:  \n
  ----------------  \n
  x - Project X     \n
  y - Project Y     \n
  z - Project Z     \n
';

SW_MAPPED=true;
case "$1" in
	x)
		workon 'x'
		cd "$HOME/projects/X"
		;;
	y)
		workon 'y'
		cd "$HOME/projects/Y"
		;;
	z)
		workon 'z'
		cd "$HOME/projects/Z"
		;;
	*)
		SW_MAPPED=false;
esac
